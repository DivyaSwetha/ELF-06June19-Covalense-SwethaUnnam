<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        <bean id="messageBean" class="com.covalense.springcore.beans.MessageBean">
        <property name="message" value="Welcome To Spring!!!"/>
        </bean>
        <bean id="employeeBean1" class="com.covalense.springcore.beans.EmployeeBean">
         <property name="id" value="1"/>
        <property name="name" value="Ram"/>
        </bean>
        <bean id="employeeBean2" class="com.covalense.springcore.beans.EmployeeBean">
         <property name="id" value="2"/>
        <property name="name" value="Seetha"/>
        </bean>
</beans>
